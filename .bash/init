# If not running interactively, don't do anything
[ -z "$PS1" ] && return

# reading the default bashrc
if [ -f /etc/bashrc ]; then
      . /etc/bashrc
fi

# Importing files
for file in ~/.bash/{exports,functions,colors,prompt,aliases,aut-complete,extras,hello}; do
	[ -r "$file" ] && [ -f "$file" ] && source "$file"
done;
unset file
